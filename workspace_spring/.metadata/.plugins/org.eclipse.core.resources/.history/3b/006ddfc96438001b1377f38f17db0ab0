<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"  
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<!--컬렉션에 setter를 주입하여 넣는다. -->
	<bean class="ex05.collection.Custom">
		<!-- 컬렉션 String타입 -->
		<property name="lists">
			<list>
				<value>홍길동</value>
				<value>이순신</value>
				<value>홍길자</value>
			</list>
		</property>
		
		<!-- 컬렉션 객체타입 -->
		<property name="vo">
			<list>
				<bean class="ex05.collection.MemberVO">
					<property name="name" value="홍길동"/>
					<property name="id" value="kkk123"/>
				</bean>
				<bean class="ex05.collection.MemberVO">
					<property name="name" value="이순신"/>
					<property name="id" value="aaa123"/>
				</bean>
				<bean class="ex05.collection.MemberVO">
					<property name="name" value="홍길자"/>
					<property name="id" value="bbb123"/>
				</bean>
			</list>
		</property>
		
		<!-- 컬랙션 맵 타입 -->
		<property name="maps">
			<map>
				<entry key="k1" value="홍길동"/>
				<entry key="k2">
					<bean class="ex05.collection.MemberVO">
						<property name="name" value="홍길동"/>
						<property name="id" value="kkk123"/>
					</bean>
				</entry>
			</map>
		</property>
		
		
	</bean>

</beans>